{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/player/*"
    },
    {
      "route": "/event/*"
    },
    {
      "route": "/brawler/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/"
    },
    "401": {
      "rewrite": "/"
    },
    "403": {
      "rewrite": "/"
    },
    "404": {
      "rewrite": "/"
    }
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}